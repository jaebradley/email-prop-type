{
  "name": "email-prop-type",
  "version": "0.0.0-development",
  "description": "React Email Prop Type",
  "keywords": [
    "react",
    "email",
    "email prop-type",
    "email prop",
    "email prop-types",
    "email props"
  ],
  "files": [
    "build"
  ],
  "browser": "build/index.js",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "scripts": {
    "build": "BABEL_ENV=production rollup -c",
    "codecov": "npx codecov",
    "commitmsg": "npx commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "gc": "commit",
    "is-build-es5": "npx es-check es5 './build/**/*.!(esm).js'",
    "lint": "npx eslint --ext .js .",
    "prepare": "npm run build",
    "semantic-release": "npx semantic-release",
    "test": "BABEL_ENV=test npx jest src/ --coverage --no-cache",
    "travis-deploy-once": "npx travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/email-prop-type.git"
  },
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/email-prop-type/issues"
  },
  "homepage": "https://github.com/jaebradley/email-prop-type#readme",
  "readme": "https://github.com/jaebradley/email-prop-type#readme",
  "dependencies": {
    "email-validator": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-angular": "^9.0.1",
    "@commitlint/prompt": "^9.0.1",
    "@commitlint/prompt-cli": "^9.0.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "codecov": "^3.7.0",
    "es-check": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "rollup": "^2.15.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-terser": "^6.1.0",
    "semantic-release": "^17.0.8",
    "travis-deploy-once": "^5.0.11"
  }
}
